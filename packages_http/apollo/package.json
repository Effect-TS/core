  "dependencies": {
    "@matechs/express": "^5.10.0",
    "@matechs/prelude": "^1.9.0"
  },
  "peerDependencies": {
    "apollo-server": "^2.12.0",
    "apollo-server-express": "^2.12.0",
    "graphql": "^14.2.1",
    "subscriptions-transport-ws": "^0.9.16"
  }
}
