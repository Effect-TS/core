{
  "name": "@matechs/logger-pino",
  "version": "1.10.0",
  "license": "MIT",
  "private": false,
  "repository": "https://github.com/mikearnaldi/matechs-effect.git",
  "scripts": {
    "build": "rimraf lib && rimraf es6 && tsc --build tsconfig.build.json && tsc --build tsconfig.es6.json && yarn import-path-rewrite",
    "postbuild": "yarn prettier --write \"./{lib,es6}/**/*.ts\"",
    "lint": "eslint . --ignore-pattern lib/ --ignore-pattern es6/ --ignore-pattern dtslint/ --ext .ts,.tsx",
    "autofix": "yarn lint --fix || true && yarn prettier --write \"./{src,demo}/**/*.ts\"",
    "import-path-rewrite": "import-path-rewrite",
    "tc": "tsc --noEmit"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/index.js",
  "module": "es6/index.js",
  "typings": "lib/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib",
    "es6"
  ],
  "dependencies": {
    "@matechs/logger": "^4.10.0",
    "@matechs/prelude": "^1.9.0",
    "@types/pino": "^6.0.0",
    "pino": "^6.2.0"
  },
  "devDependencies": {
    "@matechs/test-jest": "^1.10.0"
  }
}
