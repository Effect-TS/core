{
  "name": "@effect-ts/system",
  "version": "0.5.4",
  "license": "MIT",
  "private": false,
  "scripts": {
    "clean": "rimraf build tsbuildinfo",
    "build:lib": "tsc --build tsconfig.build.json && ts-node ../../scripts/package.ts",
    "build:es6": "tsc --build tsconfig.es6.json && ts-node ../../scripts/rewrite-esm.ts && babel build/esm/ -d build/esm/ -s --plugins annotate-pure-calls",
    "build:post-process": "ts-node ../../scripts/rewrite-source-maps.ts && ts-node ../../scripts/rewrite-locals.ts",
    "build": "yarn run build:lib && yarn run build:es6 && yarn run build:post-process",
    "lint": "eslint . --ext .ts,.tsx",
    "autofix": "yarn prettier && yarn lint --fix && yarn prettier",
    "prettier": "prettier --write \"./{src,test,demo}/**/*.ts\"",
    "tc": "tsc --noEmit"
  },
  "publishConfig": {
    "access": "public",
    "directory": "build"
  },
  "repository": "https://github.com/Matechs-Garage/matechs-effect.git",
  "sideEffects": false,
  "config": {
    "modules": [
      "Array",
      "Cause",
      "Clock",
      "Effect",
      "Either",
      "Exit",
      "Fiber",
      "FiberRef",
      "FreeAssociative",
      "Function",
      "GlobalExceptions",
      "Has",
      "Iterable",
      "Layer",
      "List",
      "Managed",
      "Map",
      "Mutable",
      "NonEmptyArray",
      "Option",
      "Promise",
      "Queue",
      "Random",
      "Ref",
      "RefM",
      "Schedule",
      "Scope",
      "Semaphore",
      "Stack",
      "Stream",
      "Stream/_internal",
      "Stream/BufferedPull",
      "Stream/Pull",
      "Stream/Push",
      "Stream/Sink",
      "Stream/Stream",
      "Stream/Take",
      "Stream/Transducer",
      "Supervisor",
      "Support",
      "Support/AtomicBoolean",
      "Support/AtomicNumber",
      "Support/AtomicReference",
      "Support/DublyLinkedList",
      "Support/ImmutableQueue",
      "Support/LinkedList",
      "Support/MutableQueue",
      "Support/OneShot",
      "Support/Scheduler",
      "Sync",
      "Utils",
      "XPure"
    ]
  }
}
