  "license": "MIT",
  "private": false,
  "repository": "https://github.com/mikearnaldi/matechs-effect.git",
  "scripts": {
    "build": "rimraf lib && rimraf es6 && tsc --build tsconfig.build.json && tsc --build tsconfig.es6.json && yarn import-path-rewrite",
    "postbuild": "yarn prettier --write \"./{lib,es6}/**/*.ts\"",
    "lint": "eslint . --ignore-pattern lib/ --ignore-pattern es6/ --ignore-pattern dtslint/ --ext .ts,.tsx",
    "autofix": "yarn lint --fix && yarn prettier --write \"./{src,demo}/**/*.ts\"",
    "import-path-rewrite": "import-path-rewrite",
    "tc": "tsc --noEmit"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/index.js",
  "module": "es6/index.js",
  "typings": "lib/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib",
    "es6"
  ],
  "dependencies": {
    "@matechs/prelude": "^1.9.0",
    "@morphic-ts/batteries": "^1.7.0",
    "fast-check": "^1.24.1",
    "fast-equals": "^2.0.0",
    "fp-ts": "^2.5.4",
    "fp-ts-contrib": "^0.1.15",
    "io-ts": "^2.2.1",
    "io-ts-types": "^0.5.6",
    "monocle-ts": "^2.1.0",
    "newtype-ts": "^0.3.4",
    "retry-ts": "^0.1.2"
  },
  "devDependencies": {
    "@matechs/test-jest": "^1.10.0"
  },
