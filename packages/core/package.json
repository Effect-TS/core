      "Classic/Iterable",
      "Classic/Ord",
      "Classic/Ordering",
      "Classic/Record",
      "Classic/Show",
      "Effect",
      "Effect/Cause",
      "Effect/Clock",
      "Effect/Exit",
      "Effect/Fiber",
      "Effect/FiberRef",
      "Effect/Layer",
      "Effect/Managed",
      "Effect/ProcessManager",
      "Effect/Promise",
      "Effect/Queue",
      "Effect/Random",
      "Effect/Ref",
      "Effect/RefM",
      "Effect/Schedule",
      "Effect/Scope",
      "Effect/Semaphore",
      "Effect/Stream",
