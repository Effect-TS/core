{
  "name": "@effect-ts/prelude",
  "version": "0.0.0",
  "license": "MIT",
  "private": false,
  "repository": "https://github.com/Effect-TS/core.git",
  "homepage": "https://www.matechs.com",
  "scripts": {
    "clean": "rimraf build tsbuildinfo",
    "build:lib": "ttsc --build  tsconfig.build.json && ts-node ../../scripts/package.ts",
    "build:es6": "ttsc --build  tsconfig.es6.json && ts-node ../../scripts/rewrite-esm.ts && babel build/esm/ -d build/esm/ -s --plugins annotate-pure-calls && ts-node ../../scripts/package-esm.ts",
    "build:post-process": "ts-node ../../scripts/rewrite-source-maps.ts && ts-node ../../scripts/rewrite-locals.ts && yarn prettier:lib",
    "build": "yarn run build:lib && yarn run build:es6 && yarn run build:post-process",
    "lint": "eslint . --ext .ts,.tsx",
    "autofix": "yarn prettier && yarn lint --fix && yarn prettier",
    "prettier": "prettier --write \"./{src,test,demo}/**/*.ts\"",
    "prettier:lib": "prettier --write \"./build/**/*.js\"",
    "tc": "tsc --noEmit",
    "demo": "ts-node -r tsconfig-paths/register"
  },
  "publishConfig": {
    "access": "public",
    "directory": "build"
  },
  "sideEffects": false,
  "dependencies": {
    "@effect-ts/hkt": "^0.0.0",
    "@effect-ts/system": "^0.11.9"
  },
  "config": {
    "modules": [
      "Any",
      "Applicative",
      "Associative",
      "AssociativeBoth",
      "AssociativeCompose",
      "AssociativeEither",
      "AssociativeFlatten",
      "Bounded",
      "Category",
      "Closure",
      "Commutative",
      "CommutativeBoth",
      "CommutativeEither",
      "Compact",
      "Compactable",
      "Contravariant",
      "Covariant",
      "CovariantWithIndex",
      "Derive",
      "DSL",
      "Equal",
      "Extend",
      "Filter",
      "Filterable",
      "FilterableWithIndex",
      "FilterMap",
      "FilterMapWithIndex",
      "FilterWithIndex",
      "Foldable",
      "FoldableWithIndex",
      "FoldMap",
      "FoldMapWithIndex",
      "ForEach",
      "ForEachWithIndex",
      "FX",
      "FX/Access",
      "FX/Fail",
      "FX/Provide",
      "FX/Run",
      "Id",
      "Identity",
      "IdentityBoth",
      "IdentityEither",
      "IdentityFlatten",
      "Invariant",
      "Inverse",
      "Monad",
      "None",
      "Ord",
      "Ordering",
      "Partition",
      "PartitionMap",
      "PartitionMapWithIndex",
      "Reduce",
      "ReduceRight",
      "ReduceRightWithIndex",
      "ReduceWithIndex",
      "Selective",
      "Separate",
      "Show",
      "ForEach",
      "ForEachWithIndex",
      "Wiltable",
      "WiltableWithIndex",
      "Witherable",
      "WitherableWithIndex"
    ]
  }
}
