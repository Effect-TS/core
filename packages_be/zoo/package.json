{
  "name": "@matechs/zoo",
  "version": "4.0.2",
  "license": "MIT",
  "private": false,
  "repository": "https://github.com/mikearnaldi/matechs-effect.git",
  "scripts": {
    "build": "rimraf build && tsc --build tsconfig.build.json && tsc --build tsconfig.es6.json && ts-node scripts/package.ts",
    "postbuild": "yarn prettier --write \"./build/**/*.ts\"",
    "lint": "eslint . --ext .ts,.tsx",
    "autofix": "yarn prettier && yarn lint --fix && yarn prettier",
    "prettier": "prettier --write \"./{src,tests,demo}/**/*.ts\"",
    "tc": "tsc --noEmit"
  },
  "publishConfig": {
    "access": "public",
    "directory": "build"
  },
  "sideEffects": false,
  "dependencies": {
    "@matechs/core": "^0.3.0",
    "@types/node-zookeeper-client": "^0.2.7",
    "node-zookeeper-client": "^1.1.0"
  }
}
