{
  "compilerOptions": {
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "declaration": true,
    "skipLibCheck": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "preserveSymlinks": true,
    "moduleResolution": "node",
    "noEmit": false,
    "lib": ["es2018"],
    "sourceMap": true,
    "strictFunctionTypes": true,
    "strict": true,
    "noImplicitReturns": false,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true,
    "noEmitOnError": false,
    "allowJs": false,
    "checkJs": false,
    "forceConsistentCasingInFileNames": true,
    "suppressImplicitAnyIndexErrors": true,
    "stripInternal": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@matechs/core/*": ["packages/core/build/*"],
      "@matechs/core": ["packages/core/build"],
      "@matechs/contrib/*": ["packages/contrib/build/*"],
      "@matechs/contrib": ["packages/contrib/build"],
      "@matechs/test/*": ["packages/test/build/*"],
      "@matechs/test": ["packages/test/build"],
      "@matechs/test-jest/*": ["packages/test-jest/build/*"],
      "@matechs/test-jest": ["packages/test-jest/build"],
      "@matechs/aio/*": ["packages/aio/build/*"],
      "@matechs/aio": ["packages/aio/build"],
      "@matechs/graceful/*": ["packages_be/graceful/build/*"],
      "@matechs/graceful": ["packages_be/graceful/build"],
      "@matechs/orm/*": ["packages_be/orm/build/*"],
      "@matechs/orm": ["packages_be/orm/build"],
      "@matechs/tracing/*": ["packages_be/tracing/build/*"],
      "@matechs/tracing": ["packages_be/tracing/build"],
      "@matechs/uuid/*": ["packages_be/uuid/build/*"],
      "@matechs/uuid": ["packages_be/uuid/build"],
      "@matechs/zoo/*": ["packages_be/zoo/build/*"],
      "@matechs/zoo": ["packages_be/zoo/build"],
      "@matechs/browser/*": ["packages_fe/browser/build/*"],
      "@matechs/browser": ["packages_fe/browser/build"],
      "@matechs/rxjs/*": ["packages_fe/rxjs/build/*"],
      "@matechs/rxjs": ["packages_fe/rxjs/build"],
      "@matechs/epics/*": ["packages_fe/epics/build/*"],
      "@matechs/epics": ["packages_fe/epics/build"]
    }
  },
  "exclude": ["node_modules", "lib"]
}
